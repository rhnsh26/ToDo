{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.0.5",
    "source-map-explorer": "^1.7.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/tailwind.css -o src/index.css",
    "watch:css": "postcss src/index.css -o src/styles/tailwind.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "postcss-cli": "^6.1.2",
    "tailwindcss": "^1.0.1"
  }
}
